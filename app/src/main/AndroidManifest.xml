<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.capstone.kumar.pupil">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:isScrollContainer="true"
            android:windowSoftInputMode="stateVisible|adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".signUp.SignUpActivity"
            android:isScrollContainer="true"
            android:windowSoftInputMode="stateVisible|adjustPan" />
        <activity android:name=".MainActivity" />
        <activity android:name=".StudentFeedback"></activity>
        <activity android:name=".AdminOperations.AdminFeedBack" ></activity>
        <activity android:name=".AdminOperations.PublishFeedBack" ></activity>
    </application>

</manifest>